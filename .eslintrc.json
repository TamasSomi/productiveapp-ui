{   "settings": {
    "react": {
    "version": "detect"
    }
},
"env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
},
"extends": [
    "eslint:recommended",
    "plugin:react/recommended"
],
"parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
},
"plugins": [
    "react"
],
"rules": {
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "req|res|next|__" }]
}

}